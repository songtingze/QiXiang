                                    <TableColumn fx:id="indexName" prefWidth="80" resizable="false" text="指标名称" />
                                    <TableColumn fx:id="indexCode" prefWidth="80" resizable="false" text="指标代码" />
                                    <TableColumn fx:id="indexData" prefWidth="80" resizable="false" text="指标临界值" />
                                    <TableColumn fx:id="indexJudge" prefWidth="80" resizable="false" text="指标判断条件" />
                                    <TableColumn fx:id="indexStatus" prefWidth="80" resizable="false" text="指标状态" />
                                </columns>
                            </MyTableView>
                        </content>
                    </ScrollPane>
                </children>
            </AnchorPane>
        </children>
