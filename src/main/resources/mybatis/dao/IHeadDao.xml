<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.dao.IHeadDao">
    <select id="queryByHid" parameterType="String" resultType="Head">
        select * from head where hid = #{hid}
    </select>
    <insert id="addHead" parameterType="Head" useGeneratedKeys="true" keyProperty="id">
        insert into head(id,hid,path) values (#{id},#{hid},#{path})
    </insert>
    <update id="updateHead" parameterType="Head">
        update head set id = #{id},hid = #{hid},path = #{path}, where hid = #{hid}
    </update>
</mapper>