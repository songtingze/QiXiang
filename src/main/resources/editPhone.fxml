<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="235.0" prefWidth="508.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.view.EditPhoneController">
    <children>
        <AnchorPane layoutX="7.0" layoutY="8.0" prefHeight="193.0" prefWidth="495.0">
            <children>
                <GridPane layoutX="96.0" layoutY="25.0" prefHeight="142.0" prefWidth="303.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="手机号" />
                        <Label text="指标判断条件" GridPane.rowIndex="1" />
                        <TextField fx:id="phone" GridPane.columnIndex="1" />
                        <ChoiceBox fx:id="status" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
            </children>
        </AnchorPane>
        <AnchorPane layoutX="2.0" layoutY="202.0" prefHeight="42.0" prefWidth="505.0" style="-fx-background-color:rgb(240,240,240);">
            <children>
                <Button fx:id="saveBtn" layoutX="333.0" layoutY="7.0" mnemonicParsing="false" onMouseClicked="#save" prefHeight="30.0" prefWidth="72.0" text="确定 " />
                <Button fx:id="cancelBtn" layoutX="421.0" layoutY="7.0" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="30.0" prefWidth="72.0" text="重置" />
            </children>
        </AnchorPane>
        <Label layoutX="-100.0" text="Label" />
    </children>
</AnchorPane>
